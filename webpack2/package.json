{
  "name": "ag-grid-angular-example",
  "version": "8.0.0",
  "description": "ag-Grid Angular 2 Example Using Webpack",
  "scripts": {
    "postinstall": "webdriver-manager update",
    "clean": "rimraf dist && rimraf app/*.js* && rimraf app/*.map",
    "start": "webpack-dev-server --inline --progress --port 8080",
    "build": "npm run clean && webpack --config config/webpack.prod.js --progress --profile --bail",
    "test": "tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
    "e2e": "protractor protractor.config.js --kill-others --success first",
    "server": "http-server -s dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ceolter/ag-grid-angular-example.git"
  },
  "author": "Niall Crosby <niall.crosby@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ceolter/ag-grid-angular-example/issues"
  },
  "homepage": "http://www.ag-grid.com/",
  "dependencies": {
    "ag-grid": "8.0.x",
    "ag-grid-enterprise": "8.0.x",
    "ag-grid-angular": "8.0.x",
    "@angular/common": "2.4.x",
    "@angular/compiler": "2.4.x",
    "@angular/compiler-cli": "2.4.x",
    "@angular/core": "2.4.x",
    "@angular/http": "2.4.x",
    "@angular/forms": "2.4.x",
    "@angular/platform-browser": "2.4.x",
    "@angular/platform-browser-dynamic": "2.4.x",
    "@angular/platform-server": "2.4.x",
    "@angular/router": "3.4.x",
    "rxjs": "5.0.x",
    "zone.js": "0.7.x",
    "core-js": "2.4.x",
    "bootstrap": "3.3.6",
    "jquery": "3.1.1"
  },
  "devDependencies": {
    "@types/node": "6.0.45",
    "@types/selenium-webdriver": "2.53.33",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.0.3",
    "canonical-path": "0.0.2",
    "concurrently": "2.2.0",
    "css-loader": "^0.23.1",
    "del": "^2.2.2",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "file-loader": "^0.8.5",
    "gulp": "3.9.1",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.15.0",
    "http-server": "^0.9.0",
    "jasmine-core": "2.4.1",
    "karma": "1.3.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-cli": "1.0.1",
    "karma-jasmine": "1.0.2",
    "karma-jasmine-html-reporter": "0.2.2",
    "protractor": "4.0.13",
    "raw-loader": "^0.5.1",
    "rimraf": "2.5.x",
    "run-sequence": "^1.2.2",
    "style-loader": "^0.13.1",
    "typescript": "2.1.x",
    "url-loader": "0.5.7",
    "webdriver-manager": "10.2.5",
    "webpack": ">=2.2.0",
    "webpack-dev-server": ">=2.2.0",
    "webpack-merge": "^2.0.0"
  }
}
