{
  "name": "ag-grid-ng2-example",
  "version": "8.0.0",
  "description": "ag-Grid Angular 2 Example Using SystemJS",
  "scripts": {
    "postinstall": "webdriver-manager update",
    "tsc": "tsc -p tsconfig.json",
    "tsc:w": "tsc -p tsconfig.json -w",
    "lite": "lite-server",
    "clean": "npm run clean:jit && npm run clean:aot",
    "clean:jit": "rimraf app/*.js*",
    "start": "npm run tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "clean:aot": "gulp clean-aot",
    "build:aot": "gulp ngc && gulp aot-bundle",
    "lite:aot": "lite-server -c aot/bs-config.json",
    "clean-build:aot": "gulp clean-aot-build",
    "test": "tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
    "e2e": "tsc && concurrently \"http-server -s\" \"protractor protractor.config.js\" --kill-others --success first"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ceolter/ag-grid-ng2-example.git"
  },
  "author": "Niall Crosby <niall.crosby@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ceolter/ag-grid-ng2-example/issues"
  },
  "homepage": "http://www.ag-grid.com/",
  "dependencies": {
    "ag-grid": "8.0.x",
    "ag-grid-enterprise": "8.0.x",
    "ag-grid-ng2": "8.0.x",
    "@angular/common": "2.4.x",
    "@angular/compiler": "2.4.x",
    "@angular/compiler-cli": "2.4.x",
    "@angular/core": "2.4.x",
    "@angular/http": "2.4.x",
    "@angular/forms": "2.4.x",
    "@angular/platform-browser": "2.4.x",
    "@angular/platform-browser-dynamic": "2.4.x",
    "@angular/platform-server": "2.4.x",
    "@angular/router": "3.4.x",
    "rxjs": "5.0.x",
    "zone.js": "0.7.x",
    "core-js": "2.4.x",
    "systemjs": "0.19.27",
    "bootstrap": "3.3.6",
    "jquery": "3.1.1"
  },
  "devDependencies": {
    "@types/node": "6.0.45",
    "@types/selenium-webdriver": "2.53.33",
    "canonical-path": "0.0.2",
    "concurrently": "2.2.0",
    "copyfiles": "1.0.x",
    "del": "^2.2.2",
    "gulp": "3.9.1",
    "gulp-ngc": "^0.1.2",
    "http-server": "^0.9.0",
    "jasmine-core": "2.4.1",
    "karma": "1.3.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-cli": "1.0.1",
    "karma-jasmine": "1.0.2",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-systemjs": "0.16.0",
    "lite-server": "2.2.2",
    "ncp": "2.0.x",
    "protractor": "4.0.13",
    "rimraf": "2.5.x",
    "run-sequence": "^1.2.2",
    "systemjs-builder": "0.15.33",
    "typescript": "2.0.3",
    "webdriver-manager": "10.2.5"
  }
}
