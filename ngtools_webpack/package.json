{
  "name": "ag-grid-angular-example",
  "version": "13.3.0",
  "description": "ag-Grid Angular Example Using Webpack 2",
  "scripts": {
    "clean": "rimraf dist && rimraf app/*.js* && rimraf app/*.map",
    "clean-aot": "rimraf dist/app* && rimraf dist/vendor* && rimraf dist/poly* && rimraf dist/index.html",
    "clean-non-aot": "rimraf dist/non-aot/*",
    "start:aot": "webpack-dev-server --config config/webpack.prod.aot.js --watch --profile --progress",
    "build:aot": "npm run clean-aot && webpack --config config/webpack.prod.aot.js --progress --profile --bail",
    "start:non-aot": "webpack-dev-server --config config/webpack.prod.non-aot.js --watch --profile --progress",
    "build:non-aot": "npm run clean-non-aot && webpack --config config/webpack.prod.non-aot.js --progress --profile --bail",
    "build":"npm run build:aot",
    "start": "webpack-dev-server --config config/webpack.dev.js --inline --progress --port 8080"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ag-grid/ag-grid-angular-example.git"
  },
  "author": "Niall Crosby <niall.crosby@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ag-grid/ag-grid-angular-example/issues"
  },
  "homepage": "http://www.ag-grid.com/",
  "dependencies": {
    "ag-grid": "13.3.x",
    "ag-grid-enterprise": "13.3.x",
    "ag-grid-angular": "13.3.x",
    "@angular/common": "4.1.3",
    "@angular/compiler": "4.1.3",
    "@angular/compiler-cli": "4.1.3",
    "@angular/core": "4.1.3",
    "@angular/http": "4.1.3",
    "@angular/forms": "4.1.3",
    "@angular/platform-browser": "4.1.3",
    "@angular/platform-browser-dynamic": "4.1.3",
    "@angular/platform-server": "4.1.3",
    "@angular/router": "4.1.3",
    "core-js": "2.4.x",
    "rxjs": "5.0.x",
    "zone.js": "0.8.16",
    "bootstrap": "3.3.6"
  },
  "devDependencies": {
    "@angular/compiler-cli": "4.1.3",
    "@ngtools/webpack": "1.6.0-rc.3",
    "@types/node": "6.0.45",
    "angular2-template-loader": "0.6.2",
    "awesome-typescript-loader": "3.1.3",
    "html-loader": "0.4.5",
    "canonical-path": "0.0.2",
    "ts-helpers": "1.1.2",
    "html-webpack-plugin": "2.28.0",
    "extract-text-webpack-plugin": "2.0.0",
    "webdriver-manager": "10.2.5",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.4.5",
    "typescript": "2.2.2",
    "rimraf": "2.5.4",
    "url-loader": "0.5.7",
    "css-loader": "0.23.1",
    "style-loader": "0.13.2",
    "raw-loader": "0.5.1",
    "toposort":"1.0.4"
  }
}

